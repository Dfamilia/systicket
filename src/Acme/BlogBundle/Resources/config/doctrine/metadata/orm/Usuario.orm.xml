<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="Apellido" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="username" type="string" column="Username" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userpass" type="string" column="UserPass" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipouser" type="string" column="TipoUser" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rolid" type="integer" column="RolID" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estado" type="string" column="Estado" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecharegistro" type="string" column="FechaRegistro" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
