<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ticket" table="ticket">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechacreado" type="string" column="FechaCreado" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechastatus" type="string" column="FechaStatus" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechacierre" type="string" column="FechaCierre" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionproblema" type="string" column="DescripcionProblema" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categoria" type="string" column="Categoria" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="Estado" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prioridad" type="string" column="Prioridad" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuariosolicitanteid" type="integer" column="UsuarioSolicitanteID" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="usuarioasignadoid" type="integer" column="UsuarioAsignadoID" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="titulo" type="string" column="Titulo" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
